type Plants {

     ID: ID,
     name: String,
     type: String,
     area: String,
     author: String,
     coast: Int,
     description: String,
     imageId: Int,
     userId: Int

}

type Images {

    imageURL: String

}

type Query {
	plants: [Plants!]!
	plant(name: String): Plants
    image(imageURL: String): Images
}

type Mutation {
	createPlant(name: String!, userId: Int!, type: String, description: String): Plants
}